@model Prectice1.CustomModels.RestaurantInfoViewInfo

@{
    ViewBag.Title = "Create";
}
<h2>@ViewBag.Message</h2>

<h2>Create</h2>


@using (Html.BeginForm())
{
   

<div class="form-horizontal">


    <div class=“form-group”>
        @Html.LabelFor(m => m.RestaurantName)
        @Html.TextBoxFor(m => m.RestaurantName, new { @class = "form-control" })
        @Html.ValidationMessageFor(m=>m.RestaurantName,"",new {@class="text-danger"})
    </div>
    <div class=“form-group”>
        @Html.LabelFor(m => m.Contact)
        @Html.TextBoxFor(m => m.Contact, new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.Contact, "", new { @class = "text-danger" })
    </div>
    <div class=“form-group”>
        @Html.LabelFor(m => m.Description)
        @Html.TextBoxFor(m => m.Description, new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.Description, "", new { @class = "text-danger" })
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Next" class="btn btn-default" />
        </div>
    </div>
</div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
